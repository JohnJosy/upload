{
  "name": "upload",
  "displayName": "upload",
  "description": "upload file to server with sftp",
  "icon":"synchronize.svg",
  "version": "0.1.0",
  "publisher": "wuwei",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlutwuwei/upload.git"
  },
  "homepage": "https://github.com/dlutwuwei/upload",
  "bugs": {
    "url": "https://github.com/dlutwuwei/upload/issues"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "upload.upload",
        "title": "upload"
      },
      {
        "command": "upload.download",
        "title": "download"
      },
      {
        "command": "upload.readdir",
        "title": "readdir"
      },
      {
        "command": "editor.upload",
        "title": "upload to server"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "editor.upload"
        }
      ],
      "explorer/context": [
        {
          "command": "editor.upload"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "ssh2": "^0.5.2",
    "vscode": "^0.11.0"
  }
}