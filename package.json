{
  "name": "upload",
  "displayName": "upload",
  "description": "upload file to server with sftp",
  "version": "0.0.6",
  "publisher": "wuwei",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "upload.upload",
        "title": "upload"
      },{
        "command": "upload.download",
        "title": "download"
      },{
        "command": "upload.readdir",
        "title": "readdir"
      },{
        "command": "editor.upload",
        "title": "upload to server"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "editor.upload"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./test/index.js"
  },
  "dependencies": {
    "ssh2": "^0.5.2",
    "vscode": "^0.11.0"
  }
}
